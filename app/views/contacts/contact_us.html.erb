<% provide :page_title, "Contact Us" %>
<div id="map_canvas" style="width: 100%; height: 390px; margin-bottom: 18px; margin-top: 18px">
</div>
<div class="page-header">
  <h1>Contact Us</h1>
</div>
<% unless @club.nil? %>
    <h3>Meetings Address:</h3>
    <address>

      <% @club.each do |c| %>
          <strong><%= c.name %></strong><br>
          <%= c.meeting_address %>
          <br>
          </address>
          <address><strong>Tel:</strong> <%= c.phone %> <br>
            <strong>Email Address: </strong> <a href="mailto:#" target="_blank"><%= c.email %></a></address>
          <h3>Mailing Address:</h3>
          <address>
            <strong><%= c.name %></strong><br>
            <%= c.mailing_address %>
      <% end %>
<% else %>
    <p class="alert alert-error">No data available!</p>
<% end %>

</address>
