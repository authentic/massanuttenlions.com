<% provide :page_title, "Webmaster" %>
<% unless @club.nil? %>
    <% @club.each do |c| %>
        <div class="page-header">

          <h1><%= @webmaster.size > 1 ? 'Webmaster'.pluralize : "Webmaster" %></h1>
        </div>
        <ul class="thumbnails">
          <% @webmaster.each do |webmaster| %>
              <li>
                <div class="thumbnail">
                  <%= image_tag webmaster.avatar.url(:medium) %>
                  <h5><%= webmaster.signature %> </h5>
                </div>
              </li>

          <% end %></ul>
        <h3>Mailing Address:</h3>
        <address>
          <strong><%= c.name %></strong><br>
          <%= c.mailing_address %>
        </address>
        <address>
          <strong>Email Address:</strong> <a href="mailto:#" target="_blank"><%= c.webmaster_email %></a></address>
    <% end %>
<% else %>
    <p class="alert alert-error">No data available!</p>
<% end %>