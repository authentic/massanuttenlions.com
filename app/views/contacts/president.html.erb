<% provide :page_title, "Club President" %>
<% unless @club.nil? %>
    <% @club.each do |c| %>
        <div class="page-header">
          <h1>Club President <%= c.current_year %></h1></div>
        <ul class="thumbnails">
          <li>
            <div class="thumbnail">
              <%= image_tag @president.avatar.url(:large) %>
              <h4><%= @president.signature %></h4>
            </div>
          </li>
        </ul>
        <h3>Mailing Address:</h3>
        <address>
          <strong><%= c.name %></strong><br>
          <%= c.mailing_address %>
        </address>
        <address><strong>Tel:</strong> <%= c.phone %> <br>
          <strong>Email Address: </strong> <a href="mailto:#" target="_blank"><%= c.president_email %></a></address>
    <% end %>
<% else %>
    <p class="alert alert-error">No data available!</p>
<% end %>